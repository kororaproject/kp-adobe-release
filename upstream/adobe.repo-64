[adobe]
name=Adobe
baseurl=http://linuxdownload.adobe.com/linux/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-adobe
exclude=AdobeReader* adobe-release*

[adobe-32bit]
name=Adobe - 32bit
baseurl=http://linuxdownload.adobe.com/linux/i386/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-adobe
exclude=AdobeReader* adobe-release*
